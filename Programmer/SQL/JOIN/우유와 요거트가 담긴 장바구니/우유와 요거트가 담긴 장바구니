--- WHERE 사용
SELECT C1.CART_ID
FROM CART_PRODUCTS C1, CART_PRODUCTS C2
WHERE C1.CART_ID = C2.CART_ID AND C1.NAME = 'Milk' AND C2.NAME = 'Yogurt'
ORDER BY C1.CART_ID

--- SUBQUERY 사용
SELECT CART_ID
FROM CART_PRODUCTS
WHERE NAME = 'Milk' AND CART_ID IN (
    SELECT CART_ID
    FROM CART_PRODUCTS
    WHERE NAME = 'Yogurt'
)
ORDER BY CART_ID

--- JOIN 사용
SELECT A.CART_ID
FROM (SELECT CART_ID FROM CART_PRODUCTS WHERE NAME = 'Milk') AS A JOIN
(SELECt CART_ID FROM CART_PRODUCTS WHERE NAME = 'Yogurt') AS B
ON A.CART_ID = B.CART_ID
ORDER BY CART_ID