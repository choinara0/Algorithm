---LIKE 사용
SELECT ANIMAL_ID, NAME
FROM ANIMAL_INS
WHERE ANIMAL_TYPE = 'Dog' AND NAME LIKE ('%el%')
ORDER BY NAME

---UPPER(LOWER)와 LIKE 사용
SELECT ANIMAL_ID, NAME
FROM ANIMAL_INS
WHERE ANIMAL_TYPE = 'Dog' AND LOWER(NAME) LIKE ('%el%')
ORDER BY NAME

--- REGEXP 사용해 정규표현식으로 풀기
SELECT ANIMAL_ID, NAME
FROM ANIMAL_INS
WHERE ANIMAL_TYPE = 'Dog' AND LOWER(NAME) REGEXP 'el'
ORDER BY NAME